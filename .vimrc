let &t_SI = "\<Esc>]50;CursorShape=1\x7"
let &t_SR = "\<Esc>]50;CursorShape=2\x7"
let &t_EI = "\<Esc>]50;CursorShape=0\x7"

set title
set bg=light
set go=a
set mouse=a
"" highlight when searching
set hlsearch
""" use OS clipboard
set clipboard+=unnamedplus
set noruler
""" always show status line
set laststatus=2
set noshowcmd
set tabstop=2
syntax on
