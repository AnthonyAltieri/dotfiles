// Place your key bindings in this file to override the defaultsauto[]
[
  // Closing editor tabs
  {
      "key": "ctrl+alt+q",
      "command": "workbench.action.closeAllEditors"
  },
  {
      "key": "cmd+k cmd+w",
      "command": "-workbench.action.closeAllEditors"
  },
  {
      "key": "shift+alt+q",
      "command": "workbench.action.closeEditorsInGroup"
  },
  {
      "key": "cmd+k w",
      "command": "-workbench.action.closeEditorsInGroup"
  },
  {
      "key": "alt+q",
      "command": "workbench.action.closeOtherEditors"
  },
  {
      "key": "alt+cmd+t",
      "command": "-workbench.action.closeOtherEditors"
  },
  // Navigate to tabs with numbers
  {
      "key": "cmd+1",
      "command": "workbench.action.openEditorAtIndex1"
    },
    {
      "key": "cmd+2",
      "command": "workbench.action.openEditorAtIndex2"
    },
    {
      "key": "cmd+3",
      "command": "workbench.action.openEditorAtIndex3"
    },
    {
      "key": "cmd+4",
      "command": "workbench.action.openEditorAtIndex4"
    },
    {
      "key": "cmd+5",
      "command": "workbench.action.openEditorAtIndex5"
    },
    {
      "key": "cmd+6",
      "command": "workbench.action.openEditorAtIndex6"
    },
    {
      "key": "cmd+7",
      "command": "workbench.action.openEditorAtIndex7"
    },
    {
      "key": "cmd+8",
      "command": "workbench.action.openEditorAtIndex8"
    },
    {
      "key": "cmd+9",
      "command": "workbench.action.openEditorAtIndex9"
    },
    // Explorer keyboard shortcuts
  { 
      "key": "cmd+n", 
      "command": "explorer.newFile", 
      "when": "explorerViewletFocus" 
  },
  { 
      "key": "shift+cmd+n", 
      "command": "explorer.newFolder", 
      "when": "explorerViewletFocus" 
  },
  // Jupyter
  {
    "key": "ctrl+shift+cmd+r",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "jupyter.restartkernel",
        "jupyter.notebookeditor.runallcells",
        "notebook.clearAllCellsOutputs"
      ]
    }
  },
  {
    "key": "o",
    "command": "-notebook.cell.toggleOutputs",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "o",
    "command": "-list.toggleExpand",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "o",
    "command": "notebook.cell.insertCodeCellBelowAndFocusContainer",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "b",
    "command": "-notebook.cell.insertCodeCellBelowAndFocusContainer",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "shift+o",
    "command": "notebook.cell.insertCodeCellAboveAndFocusContainer",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "a",
    "command": "-notebook.cell.insertCodeCellAboveAndFocusContainer",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "s l",
    "command": "workbench.action.navigateRight",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "s h",
    "command": "workbench.action.navigateLeft",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "s j",
    "command": "workbench.action.navigateDown",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "s k",
    "command": "workbench.action.navigateUp",
    "when": "notebookEditorFocused && !inputFocus"
  },
  // known bug, this requires two tabs even though we have 1 here for some reason
  {
    "key": "tab",
    "command": "workbench.action.nextEditorInGroup",
    "when": "notebookEditorFocused && !inputFocus"
  },
  {
    "key": "shift+tab",
    "command": "workbench.action.previousEditorInGroup",
    "when": "notebookEditorFocused && !inputFocus"
  },
  // Imports
  {
    "key": "shift+alt+o",
    "command": "-editor.action.organizeImports",
    "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/"
  },
  // typescript
  {
    "key": "shift+cmd+o",
    "command": "-workbench.action.gotoSymbol"
  },
  {
    "key": "ctrl+cmd+o",
    "command": "extension.multiCommand.execute",
    "args": {
      "interval": 100,
      "sequence": [
        "typescript.removeUnusedImports",
        "typescript.sortImports",
      ]
    },
    "when": "editorTextFocus"
  },
  // toggle signature help
  {
    "key": "alt+h",
    "command": "editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "alt+h",
    "command": "closeParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus && parameterHintsVisible"
  },
  // show hover with keyboard
  {
    "key": "alt+v",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  // Extension: VSCode Harpoon
  {
    "key": "alt+h",
    "command": "vscode-harpoon.editEditors",
    "when": "editGlobalEditors"
  }
]